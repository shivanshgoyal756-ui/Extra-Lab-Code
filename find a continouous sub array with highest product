#include <stdio.h>

int main() {
   int n;
   printf("\nEnter how many numbers are to be inputed ");
   scanf("%d",&n);
   int A[n];
   
   for(int i=0;i<n;++i)
   {
       printf("\nEnter the number");
       scanf("%d",&A[i]);
   }

   
   int sum=1;
   int st=0;
   int end=0;
   int large=A[0];
   
   for(int i=0;i<n;++i)
   {
       for(int j=i;j<n;++j)
       {
           sum=sum*A[j];
           if(sum>large)
           {
               large=sum;
               st=i;
               end=j;
           }
           
       }
       
       sum=0;
       
   }
   printf("\n%d",sum);
   for(int i=st;i<=end;++i)
   {
       printf("%d  ",A[i]);
   }
    return 0;
}
