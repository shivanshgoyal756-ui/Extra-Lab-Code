#include <stdio.h>

int main() {
   int n;
   printf("\nEnter how many numbers are to be inputed ");
   scanf("%d",&n);
   int A[n];
   int num;
   printf("\nEnter the sum to be searched");
   scanf("%d",&num);
   for(int i=0;i<n;++i)
   {
       printf("\nEnter the number");
       scanf("%d",&A[i]);
   }

   int c=0;
   int sum=0;
   int st=0;
   int end=0;
   for(int i=0;i<n;++i)
   {
       for(int j=i;j<n;++j)
       {
           sum=sum+A[j];
           if(sum==num)
           {
               c=1;
               st=i;
               end=j;
               break;
               
           }
           
       }
       if(c==1)
       break;
       sum=0;
       
   }
   for(int i=st;i<=end;++i)
   {
       printf("%d  ",A[i]);
   }
    return 0;
}
