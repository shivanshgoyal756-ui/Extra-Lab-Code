#include <stdio.h>

  int main() {
      int num;
      printf("Enter how many number are to be input");
    scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
     int cc[num];
     for(int j=0;j<=num-1;++j)
    {
       cc[j]=n[j];
    }
     for(int a=0;a<num;++a)
     {
         printf("%d ",cc[a]);
     }
    return 0;
  }
  
