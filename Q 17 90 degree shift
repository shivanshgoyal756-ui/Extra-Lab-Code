#include <stdio.h>

int main() {
 int n;
 printf("Enter the row or column number");
 scanf("%d",&n);
 int A[n][n];
 for(int i=0;i<n;++i)
 {
     for(int j=0;j<n;++j)
     {
         printf("\nEnter the number");
         scanf("%d",&A[i][j]);
     }
 }
 for(int i=n-1;i>=0;--i)
 {
     for(int j=0;j<n;++j)
     {
         printf("%d ",A[j][i]);
     }
     printf("\n");
 }
 return 0;
}
