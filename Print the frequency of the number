#include <stdio.h>
#include <math.h>

int main() {
    int num;
    int tt;
    printf("Enter the number");
    scanf("%d",&num);
    int digits=(int)log10(num);
    digits=digits+1;
    int ff[digits];
    int rr[digits];
    int dd=0;
    int b=0;
    int c=1;
    ff[0]=num%10;
    rr[0]=1;
    num=num/10;
    tt=1;
     for(int z=1;z<=digits;++z)
    {
       ff[z]=0;
       rr[z]=0;
    }
    while(num>0)
    {
        dd=num%10;
        for(int i=0;i<c;++i)
        {
            if(dd==ff[i])
            {
            rr[i]=rr[i]+1;
            b=1;
            }
        }
        if(b==0)
        {
            
            ff[c]=dd;
            rr[c]=1;
            c=c+1;
            tt=tt+1;
        }
        b=0;
        num=num/10;
        
    }
    for(int a=0;a<tt;++a)
    {
        printf("\nthe number is %d   frequency is %d",ff[a],rr[a]);
    }
    return 0;
    
    }
