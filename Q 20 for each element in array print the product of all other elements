#include <stdio.h>

int main() {
 int n;
 printf("Enter how many numbers are to be inputed");
 scanf("%d",&n);
 int a[n];
 for(int i=0;i<n;++i)
 {
     printf("Enter the number");
     scanf("%d",&a[i]);
 }
 int sum=1;
 for(int i=0;i<n;++i)
 {
     for(int j=0;j<n;++j)
     {
         if(i!=j)
         sum=sum*a[j];
     }
     printf("%d ",sum);
     sum=1;
 }
 
 return 0;
}
