#include <stdio.h>

  int main() {
      int num;
      int nn;
      printf("Enter how many number are to be input 1st");
    scanf("%d",&num);
    int n[num];
    
     for(int i=0;i<=num-1;++i)
    {
        printf("num");
        scanf("%d",&n[i]);
    }
    printf ("enter the numbers to be inserted in 2nd ");
    scanf("%d",&nn);
     int cc[nn];
     
     for(int j=0;j<=nn-1;++j)
    {
       printf("num");
        scanf("%d",&cc[j]);
    }
    if(nn<=num)
     {
         for(int a=0;a<nn;++a)
     {
        if(n[a]=cc[a])
        {
            printf("%d ",n[a]);
        }
        else
        ;
         
     }
     }
     else
      for(int b=0;b<num;++b)
     {
        if(n[b]=cc[b])
        {
        printf("%d ",n[b]);
        }
     }
     
    return 0;
  }
  
